		"postcss": "^8.4.35",
		"prettier-plugin-tailwindcss": "^0.5.13",
		"tailwindcss": "^3.4.1",
		"typescript": "^5.3.3",
		"vite": "^5.1.5",
		"vite-svg-loader": "^5.1.0",
		"vue-tsc": "^2.0.4"
